          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="server=192.168.1.7;User Id=sa; Password=Ne7f5fxv!;database=CuriousPeach" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
